{
  "name": "angular7-electron-sqlite3-bootstrap4-webpack4",
  "productName": "Angular7-Electron-Sqlite3-Bootstrap4-Webpack4+",
  "version": "1.0.6",
  "description": "An Electron application using Angular 7+, Sqlite3, Bootstrap 4+ and WebPack 4+",
  "author": "pamtbaau",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "test": "karma start ./config/karma.conf.js",
    "build": "webpack --config config/webpack.dev.js --watch",
    "build:once": "webpack --config config/webpack.dev.js",
    "build:prod": "webpack --config config/webpack.prod.js",
    "postinstall": "electron-builder install-app-deps",
    "package": "electron-builder ."
  },
  "license": "MIT",
  "build": {
    "appId": "com.pamtbaau.angular7-electron-sqlite3-bootstrap4-webpack4",
    "productName": "Angular7-Electron-Sqlite3-Bootstrap4-Webpack4",
    "directories": {
      "output": "build"
    },
    "nsis": {
      "oneClick": false
    },
    "files": [
      "!**/.vscode",
      "!**/build",
      "!**/config",
      "!**/assets/data",
      "!**/src"
    ],
    "extraResources": [
      {
        "from": "src/assets/data",
        "to": "dist/assets/data",
        "filter": "database*"
      }
    ]
  },
  "dependencies": {
    "@angular/common": "^7.1.2",
    "@angular/compiler": "^7.1.2",
    "@angular/core": "^7.1.2",
    "@angular/forms": "^7.1.2",
    "@angular/http": "^7.1.2",
    "@angular/platform-browser": "^7.1.2",
    "@angular/platform-browser-dynamic": "^7.1.2",
    "bootstrap": "^4.1.3",
    "core-js": "^2.6.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.6",
    "rxjs": "^6.3.3",
    "sqlite3": "^4.0.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@types/core-js": "^2.5.0",
    "@types/jasmine": "^3.3.0",
    "@types/node": "^10.12.12",
    "@types/sqlite3": "^3.1.3",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^9.4.2",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^4.6.0",
    "electron": "^3.0.10",
    "electron-builder": "^20.38.2",
    "electron-reload": "^1.4.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-core": "^3.3.0",
    "karma": "3.1.3",
    "karma-electron": "^6.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.5",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-command": "0.4.2",
    "webpack-merge": "^4.1.4"
  }
}
