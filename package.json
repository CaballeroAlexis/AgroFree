{
  "name": "angular5-electron-sqlite3-bootstrap4-webpack",
  "productName": "Angular5-Electron-Sqlite3-Bootstrap4-Webpack",
  "version": "1.0.0",
  "description": "An Electron application using Angular 5+, Sqlite3, Bootstrap 4 and WebPack",
  "author": "pamtbaau",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "test": "karma start ./config/karma.conf.js",
    "build": "webpack --config config/webpack.dev.js --watch",
    "build:once": "webpack --config config/webpack.dev.js",
    "build:prod": "webpack --config config/webpack.prod.js",
    "postinstall": "electron-builder install-app-deps",
    "package": "electron-builder ."
  },
  "license": "MIT",
  "build": {
    "appId": "com.pamtbaau.angular5-electron-sqlite3-bootstrap4-webpack",
    "productName": "Angular5-Electron-Sqlite3-Bootstrap4-Webpack",
    "directories": {
      "output": "build"
    },
    "nsis": {
      "oneClick": false
    },
    "files": [
      "!**/.vscode",
      "!**/build",
      "!**/config",
      "!**/assets/data",
      "!**/src"
    ],
    "extraResources": [
      {
        "from": "src/assets/data",
        "to": "dist/assets/data",
        "filter": "database*"
      }
    ]
  },
  "dependencies": {
    "@angular/common": "^5.1.2",
    "@angular/compiler": "^5.1.2",
    "@angular/core": "^5.1.2",
    "@angular/forms": "^5.1.2",
    "@angular/http": "^5.1.2",
    "@angular/platform-browser": "^5.1.2",
    "@angular/platform-browser-dynamic": "^5.1.2",
    "bootstrap": "^4.0.0-beta",
    "core-js": "^2.5.3",
    "jquery": "^3.2.1",
    "popper.js": "^1.13.0",
    "rxjs": "^5.5.5",
    "sqlite3": "^3.1.13",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.43",
    "@types/jasmine": "^2.8.2",
    "@types/node": "^8.5.2",
    "@types/sqlite3": "^3.1.1",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.2.3",
    "awesome-typescript-loader": "^3.4.1",
    "copy-webpack-plugin": "^4.3.0",
    "css-loader": "^0.28.7",
    "electron": "^1.7.10",
    "electron-builder": "^19.49.2",
    "electron-reload": "^1.2.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "jasmine-core": "^2.8.0",
    "karma": "1.7.1",
    "karma-electron": "^5.2.2",
    "karma-jasmine": "^1.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.9",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "precss": "^2.0.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  }
}
